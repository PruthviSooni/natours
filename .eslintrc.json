{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "spaced-comment": "off",
        "no-console": "off",
        "no-trailing-spaces": "off",
        "indent": "off",
        "comma-dangle": "off",
        "comma-spacing": "off",
        "object-curly-newline": "off",
        "object-curly-spacing": "off",
        "key-spacing": "off",
        "keyword-spacing": "off",
        "block-spacing": "off",
        "semi-spacing": "off",
        "func-call-spacing": "off",
        "no-unused-vars": "warn",
        "no-unused-expressions": "warn",
        "padded-blocks": "off",

    }
}